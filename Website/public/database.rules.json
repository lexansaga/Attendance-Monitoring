{
  "rules": {
    ".read": true,
    ".write": true,
    "Attendance": {
      "Gate": {
        "$date": {
          "$uid": {
            ".indexOn": "EnteredID"
          }
        }
      },
      "Student": {
        ".indexOn": "Attendance_Status"
      },
      "Summary": {
        "Class": {
          ".indexOn": "Professor"
        }
      }
    },
    "Data": {
      "Faculty": {
        "Information": {
          ".indexOn": "Card_ID"
        }
      },
      "Student": {
        "Information": {
          ".indexOn": "Card_ID"
        }
      },
      "Subject": {
        ".indexOn": [
          "Professor",
          "ClassNbr"
        ]
      }
    },
    "User": {
      ".indexOn": "UserID"
    }
  }
}
